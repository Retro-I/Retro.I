{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "GpioPinMapping",
    "type": "object",
    "properties": {
        "ROTARY_VOLUME_UP": {
            "type": "integer",
            "description": "Pin for volume up",
            "default": 6
        },
        "ROTARY_VOLUME_DOWN": {
            "type": "integer",
            "description": "Pin for volume down",
            "default": 12
        },
        "ROTARY_VOLUME_PRESS": {
            "type": "integer",
            "description": "Pin for muting volume",
            "default": 13
        },
        "ROTARY_BASS_UP": {
            "type": "integer",
            "description": "Pin for bass up",
            "default": 11
        },
        "ROTARY_BASS_DOWN": {
            "type": "integer",
            "description": "Pin for bass down",
            "default": 8
        },
        "ROTARY_TREBLE_UP": {
            "type": "integer",
            "description": "Pin for treble up",
            "default": 4
        },
        "ROTARY_TREBLE_DOWN": {
            "type": "integer",
            "description": "Pin for treble down",
            "default": 14
        },
        "START_PARTY_MODE_BUTTON": {
            "type": "integer",
            "description": "Pin for button to start party mode",
            "default": 21
        }
    },
    "required": [
        "ROTARY_VOLUME_UP",
        "ROTARY_VOLUME_DOWN",
        "ROTARY_VOLUME_PRESS",
        "ROTARY_BASS_UP",
        "ROTARY_BASS_DOWN",
        "ROTARY_TREBLE_UP",
        "ROTARY_TREBLE_DOWN",
        "START_PARTY_MODE_BUTTON"
    ],
    "additionalProperties": false
}
